{
  "name": "shy",
  "private": true,
  "version": "0.1.0",
  "description": "Schoolhouse Yoga's Website",
  "author": "Nate Stuyvesant",
  "license": "https://www.schoolhouseyoga.com/terms",
  "repository": {
    "type": "git",
    "url": "https://github.com/nstuyvesant/shy3.git"
  },
  "bugs": {
    "url": "https://github.com/nstuyvesant/shy3/issues"
  },
  "homepage": "https://github.com/nstuyvesant/shy3",
  "scripts": {
    "serve": "vue-cli-service serve --open --host localhost",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.0-rc.28",
    "braintree": "^2.19.0",
    "braintree-web": "^3.51.0",
    "chart.js": "^2.8.0",
    "date-fns": "^2.0.1",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.2.2",
    "vue-router": "^3.1.3",
    "vuelidate": "^0.7.4"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.22",
    "@fortawesome/free-brands-svg-icons": "^5.10.2",
    "@fortawesome/free-regular-svg-icons": "^5.10.2",
    "@fortawesome/free-solid-svg-icons": "^5.10.2",
    "@fortawesome/vue-fontawesome": "^0.1.7",
    "@types/braintree-web": "^3.47.0",
    "@types/jest": "^24.0.18",
    "@types/vuelidate": "^0.7.7",
    "@types/bootstrap": "4.3.1",
    "@types/chart.js": "2.8.4",
    "@vue/cli-plugin-e2e-cypress": "^3.11.0",
    "@vue/cli-plugin-pwa": "^3.11.0",
    "@vue/cli-plugin-typescript": "^3.11.0",
    "@vue/cli-plugin-unit-jest": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "fibers": "^4.0.1",
    "glob-all": "^3.1.0",
    "path": "^0.12.7",
    "purgecss-webpack-plugin": "^1.5.0",
    "sass": "^1.22.10",
    "sass-loader": "~7.3.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.6.2",
    "vue-cli-plugin-pug": "^1.0.7",
    "vue-template-compiler": "^2.6.10",
    "webpack-bundle-analyzer": "^3.4.1"
  },
  "engines": {
    "node": "~12.9.1",
    "npm": "~6.11.2"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie < 11"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  }
}
